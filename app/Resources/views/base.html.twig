            <script src="{{ asset('bundles/boletines/javascript/materialize.js')}}"></script>
            <script src="{{ asset('bundles/boletines/javascript/init.js')}}"></script>
            <script src="{{ asset('bundles/boletines/javascript/main.js')}}"></script>

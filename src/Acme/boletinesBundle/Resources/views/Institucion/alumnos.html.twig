{% extends '::base.html.twig' %}

{% block body -%}

    <header>
        {{ include('BoletinesBundle::Institucion/fixed_nav.html.twig') }}
        {{ include('BoletinesBundle::Institucion/main_nav.html.twig', {'nav_selected': 'instituciones'}) }}
    </header>



    <!-- startMain content-->
    <div class="container">
        <div class="row mar-top-soft">
            <div class="col s12 m12 l12">
                <!--  Tables Section-->
                <div id="striped">
                    <div class="row">
                        <div class="col s10 m10 l10">
                            <h3 class="header">Alumnos de la Institucion</h3>

                            <div class="row">
                                <div class="col s12">

                                    <table class="striped">
                                        <thead>
                                        <tr>
                                            <th data-field="id">Nombre</th>
                                            <th data-field="id">Apellido</th>
                                            <th data-field="id">DNI</th>
                                            <th data-field="id">Tel</th>
                                            <th data-field="id">GÃ©nero</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        {% for entity in alumnos %}
                                            <tr>
                                                <td>
                                                    {{ entity.nombre }}
                                                </td>
                                                <td>
                                                    {{ entity.apellido }}
                                                </td>
                                                <td>
                                                    {{ entity.dni }}
                                                </td>
                                                <td>
                                                    {{ entity.telefono }}
                                                </td>
                                                <td>
                                                    {{ entity.sexo }}
                                                </td>
                                            </tr>
                                        {% endfor %}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                    <!--  End Tables Section  -->

                </div>
            </div>
        </div>
    </div>

    <!-- end Main content-->

    {{ include('::footer.html.twig') }}


{% endblock %}

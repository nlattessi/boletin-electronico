{% extends '::base.html.twig' %}

{% block body -%}
    <h1>GrupoUsuario list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id GrupoUsuario</th>
                <th>Nombre grupoUsuario</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for grupoUsuario in entities %}
            <tr>
                <td>{{ grupoUsuario.idGrupoUsuario }}</td>
                <td><a href="{{ path('grupoUsuario_show', { 'id': grupoUsuario.idGrupoUsuario }) }}">{{ grupoUsuario.__toString() }}</a></td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('grupoUsuario_show', { 'id': grupoUsuario.idGrupoUsuario }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('grupoUsuario_edit', { 'id': grupoUsuario.idGrupoUsuario }) }}">Editar</a>
                    </li>
                    <li>
                        <a href="{{ path('grupoUsuario_delete', { 'id': grupoUsuario.idGrupoUsuario }) }}">Borrar</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('grupoUsuario_new') }}">
                Crear Nueva GrupoUsuario
            </a>
        </li>
    </ul>
    {% endblock %}

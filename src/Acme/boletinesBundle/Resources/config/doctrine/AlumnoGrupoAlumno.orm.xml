<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\AlumnoGrupoAlumno" table="alumno_grupo_alumno">
    <indexes>
      <index name="alumno_fk_grupo_alumno_idx" columns="id_alumno"/>
      <index name="grupo_alumno_fk_alumno_idx" columns="id_grupo"/>
    </indexes>
    <id name="idAlumnoGrupoAlumno" type="integer" column="id_alumno_grupo_alumno">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idGrupo" target-entity="GrupoAlumno">
      <join-columns>
        <join-column name="id_grupo" referenced-column-name="id_grupo_alumno"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAlumno" target-entity="Alumno">
      <join-columns>
        <join-column name="id_alumno" referenced-column-name="id_alumno"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

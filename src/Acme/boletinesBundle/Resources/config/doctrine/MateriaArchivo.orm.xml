<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\MateriaArchivo" table="materia_archivo">
    <indexes>
      <index name="materia_fk_archivo" columns="id_materia"/>
      <index name="archivo_fk_materia" columns="id_archivo"/>
    </indexes>
    <id name="idMateriaArchivo" type="integer" column="id_materia_archivo">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idMateria" target-entity="Materia">
      <join-columns>
        <join-column name="id_materia" referenced-column-name="id_materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idArchivo" target-entity="Archivo">
      <join-columns>
        <join-column name="id_archivo" referenced-column-name="id_archivo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

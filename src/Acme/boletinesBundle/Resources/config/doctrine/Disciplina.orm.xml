<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Disciplina" table="disciplina">
    <indexes>
      <index name="alumno_fk_disciplina" columns="id_alumno"/>
      <index name="docente_fk_disciplina" columns="id_docente"/>
    </indexes>
    <id name="idDisciplina" type="integer" column="id_disciplina">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="comentarioDocente" type="string" column="comentario_docente" length="255" nullable="false"/>
    <field name="descargoAlumno" type="string" column="descargo_alumno" length="255" nullable="true"/>
    <field name="fechaSuceso" type="datetime" column="fecha_suceso" nullable="false"/>
    <field name="fechaCarga" type="datetime" column="fecha_carga" nullable="false"/>
    <field name="validado" type="boolean" column="validado" nullable="true"/>
    <many-to-one field="idDocente" target-entity="Docente">
      <join-columns>
        <join-column name="id_docente" referenced-column-name="id_docente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAlumno" target-entity="Alumno">
      <join-columns>
        <join-column name="id_alumno" referenced-column-name="id_alumno"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Actividad" table="actividad">
    <indexes>
      <index name="usuario_fk_actividad" columns="id_usuario_creador"/>
      <index name="archivo_fk_actividad" columns="id_archivo"/>
    </indexes>
    <id name="idActividad" type="integer" column="id_actividad">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreActividad" type="string" column="nombre_actividad" length="45" nullable="false"/>
    <field name="descripcionActividad" type="string" column="descripcion_actividad" length="500" nullable="false"/>
    <field name="fechaDesde" type="datetime" column="fecha_desde" nullable="false"/>
    <field name="fechaHasta" type="datetime" column="fecha_hasta" nullable="false"/>
    <field name="fechaCreacion" type="datetime" column="fecha_creacion" nullable="false"/>
    <many-to-one field="idUsuarioCreador" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario_creador" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idArchivo" target-entity="Archivo">
      <join-columns>
        <join-column name="id_archivo" referenced-column-name="id_archivo"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

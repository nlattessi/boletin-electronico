<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\AlumnoAsistencia" table="alumno_asistencia">
    <indexes>
      <index name="alumno_fk_asistencia" columns="id_alumno"/>
      <index name="asistencia_fk_alumno" columns="id_asistencia"/>
      <index name="justificacion_fk_alumno" columns="id_justificacion"/>
    </indexes>
    <id name="idAlumnoAsistencia" type="integer" column="id_alumno_asistencia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="valorAsistencia" type="string" column="valor_asistencia" length="1" nullable="false"/>
    <many-to-one field="idJustificacion" target-entity="Justificacion">
      <join-columns>
        <join-column name="id_justificacion" referenced-column-name="id_justificacion"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAsistencia" target-entity="Asistencia">
      <join-columns>
        <join-column name="id_asistencia" referenced-column-name="id_asistencia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAlumno" target-entity="Alumno">
      <join-columns>
        <join-column name="id_alumno" referenced-column-name="id_alumno"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Calificacion" table="calificacion">
    <indexes>
      <index name="alumno_fk_examen_idx" columns="id_alumno"/>
      <index name="examen_fk_calificacion_idx" columns="id_examen"/>
    </indexes>
    <id name="idCalificacion" type="integer" column="id_calificacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechaCalificacion" type="datetime" column="fecha_calificacion" nullable="true"/>
    <field name="valorCalificacion" type="string" column="valor_calificacion" length="10" nullable="true"/>
    <field name="comentarioCalificacion" type="string" column="comentario_calificacion" length="127" nullable="true"/>
    <field name="validada" type="string" column="validada" length="1" nullable="false"/>
    <many-to-one field="idExamen" target-entity="Examen">
      <join-columns>
        <join-column name="id_examen" referenced-column-name="id_examen"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAlumno" target-entity="Alumno">
      <join-columns>
        <join-column name="id_alumno" referenced-column-name="id_alumno"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Archivo" table="archivo">
    <indexes>
      <index name="usuario_fk_archivo" columns="id_usuario_carga"/>
    </indexes>
    <id name="idArchivo" type="integer" column="id_archivo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreParaMostrar" type="string" column="nombre_para_mostrar" length="45" nullable="false"/>
    <field name="nombreArchivo" type="string" column="nombre_archivo" length="45" nullable="false"/>
    <field name="rutaArchivo" type="string" column="ruta_archivo" length="75" nullable="false"/>
    <field name="fechaSubida" type="datetime" column="fecha_subida" nullable="false"/>
    <many-to-one field="idUsuarioCarga" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario_carga" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

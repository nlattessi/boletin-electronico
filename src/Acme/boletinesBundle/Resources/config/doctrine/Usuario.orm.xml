<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Usuario" table="usuario">
    <id name="idUsuario" type="integer" column="id_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreUsuario" type="string" column="nombre_usuario" length="45" nullable="false"/>
    <field name="password" type="string" column="password" length="45" nullable="false"/>
    <field name="nombreUsuarioParaMostrar" type="string" column="nombre_usuario_para_mostrar" length="45" nullable="false"/>
    <field name="nombreReal" type="string" column="nombre_real" length="45" nullable="true"/>
    <field name="telefonoUsuario" type="string" column="telefono_usuario" length="15" nullable="true"/>
    <many-to-one field="idRol" target-entity="Rol">
      <join-columns>
        <join-column name="id_rol" referenced-column-name="id_rol"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

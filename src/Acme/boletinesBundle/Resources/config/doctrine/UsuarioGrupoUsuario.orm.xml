<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\UsuarioGrupoUsuario" table="usuario_grupo_usuario">
    <indexes>
      <index name="usario_fk_grupo_usuario" columns="id_usuario"/>
      <index name="grupo_usuario_fk_usuario" columns="id_grupo_usuario"/>
    </indexes>
    <id name="idUsuarioGrupoUsuario" type="integer" column="id_usuario_grupo_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idGrupoUsuario" target-entity="GrupoUsuario">
      <join-columns>
        <join-column name="id_grupo_usuario" referenced-column-name="id_grupo_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

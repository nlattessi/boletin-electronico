<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\UsuarioRol" table="usuario_rol">
    <indexes>
      <index name="usuario_fk_rol" columns="id_usuario"/>
      <index name="rol_fk_usuario" columns="id_rol"/>
    </indexes>
    <id name="idUsuarioRol" type="integer" column="id_usuario_rol">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idRol" target-entity="Rol">
      <join-columns>
        <join-column name="id_rol" referenced-column-name="id_rol"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

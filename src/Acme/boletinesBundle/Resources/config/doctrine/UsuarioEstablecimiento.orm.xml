<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\UsuarioEstablecimiento" table="usuario_establecimiento">
    <indexes>
      <index name="usuario_fk_establecimiento" columns="id_usuario"/>
      <index name="establecimiento_fk_usuario" columns="id_establecimiento"/>
    </indexes>
    <id name="idUsuarioEstablecimiento" type="integer" column="id_usuario_establecimiento">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idEstablecimiento" target-entity="Establecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id_establecimiento"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

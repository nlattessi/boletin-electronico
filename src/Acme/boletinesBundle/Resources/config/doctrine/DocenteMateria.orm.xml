<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\DocenteMateria" table="docente_materia">
    <indexes>
      <index name="docente_fk_materia_idx" columns="id_docente"/>
      <index name="materia_fk_docente_idx" columns="id_materia"/>
    </indexes>
    <id name="idDocenteMateria" type="integer" column="id_docente_materia">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idMateria" target-entity="Materia">
      <join-columns>
        <join-column name="id_materia" referenced-column-name="id_materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDocente" target-entity="Docente">
      <join-columns>
        <join-column name="id_docente" referenced-column-name="id_docente"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

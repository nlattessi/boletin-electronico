<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Materia" table="materia">
    <indexes>
      <index name="calendario_fk_materia" columns="id_calendario_materia"/>
      <index name="tipo_materia_fk_materia" columns="id_tipo_materia"/>
    </indexes>
    <id name="idMateria" type="integer" column="id_materia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreMateria" type="string" column="nombre_materia" length="45" nullable="false"/>
    <many-to-one field="idTipoMateria" target-entity="TipoMateria">
      <join-columns>
        <join-column name="id_tipo_materia" referenced-column-name="id_tipo_materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCalendarioMateria" target-entity="Calendario">
      <join-columns>
        <join-column name="id_calendario_materia" referenced-column-name="id_calendario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

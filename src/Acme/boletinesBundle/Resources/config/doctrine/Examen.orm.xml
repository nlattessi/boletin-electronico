<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Examen" table="examen">
    <indexes>
      <index name="docente_fk_examen" columns="id_docente"/>
      <index name="materia_fk_examen" columns="id_materia"/>
      <index name="actividad_fk_examen" columns="id_actividad"/>
    </indexes>
    <id name="idExamen" type="integer" column="id_examen">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreExamen" type="string" column="nombre_examen" length="45" nullable="false"/>
    <field name="fechaExamen" type="datetime" column="fecha_examen" nullable="false"/>
    <many-to-one field="idMateria" target-entity="Materia">
      <join-columns>
        <join-column name="id_materia" referenced-column-name="id_materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idDocente" target-entity="Docente">
      <join-columns>
        <join-column name="id_docente" referenced-column-name="id_docente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idActividad" target-entity="Actividad">
      <join-columns>
        <join-column name="id_actividad" referenced-column-name="id_actividad"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Notificacion" table="notificacion">
    <indexes>
      <index name="usuario_envia_fk_notificacion" columns="id_usuario_envia"/>
      <index name="grupo_usuario_fk_notificacion" columns="id_grupo_usuario_recibe"/>
    </indexes>
    <id name="idNotificacion" type="integer" column="id_notificacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tituloNotificacion" type="string" column="titulo_notificacion" length="100" nullable="false"/>
    <field name="textoNotificacion" type="string" column="texto_notificacion" length="500" nullable="false"/>
    <field name="fechaEnvio" type="datetime" column="fecha_envio" nullable="false"/>
    <many-to-one field="idUsuarioEnvia" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario_envia" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idGrupoUsuarioRecibe" target-entity="GrupoUsuario">
      <join-columns>
        <join-column name="id_grupo_usuario_recibe" referenced-column-name="id_grupo_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

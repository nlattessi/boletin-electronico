<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\boletinesBundle\Entity\Asistencia" table="asistencia">
    <indexes>
      <index name="materia_fk_asistencia" columns="id_materia"/>
      <index name="usuario_fk_asistencia" columns="id_usuario_cargador"/>
    </indexes>
    <id name="idAsistencia" type="integer" column="id_asistencia">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechaAsistencia" type="datetime" column="fecha_asistencia" nullable="false"/>
    <field name="fechaCarga" type="datetime" column="fecha_carga" nullable="false"/>
    <many-to-one field="idUsuarioCargador" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario_cargador" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMateria" target-entity="Materia">
      <join-columns>
        <join-column name="id_materia" referenced-column-name="id_materia"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
